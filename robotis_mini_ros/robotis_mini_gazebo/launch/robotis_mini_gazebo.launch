<launch>

  <param name="robot_description" command="$(find xacro)/xacro.py '$(find robotis_mini_description)/urdf/robotis_mini.urdf.xacro'" />
 
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="worlds/empty.world"/>
    <arg name="paused" value="true"/>
  </include>
 
  
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen" 
      args="-z 0.17 -urdf -param robot_description -model robotis_mini" />

 
 <!-- <node name="robotis_mini_walker" pkg="robotis_mini_gazebo" type="walker.py" output="screen" /> -->
</launch>
