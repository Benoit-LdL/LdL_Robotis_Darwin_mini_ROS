<launch>
  <!-- bridge connecting to the real robot using poppy and that acts as joint state publisher -->
  <node name="robotis_mini_poppy_joint_state_publisher" pkg="robotis_mini_description" type="poppy_node.py" output="screen">
    <param name="creature" value="robotis-mini-poppy" />
    <remap from="/robotis_mini_poppy_bridge/motors/read_present" to="joint_states" />
    <!-- <remap from="/robotis_mini_poppy_bridge/motors/write" to="goal"/> -->
  </node>
</launch>
